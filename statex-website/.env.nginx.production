# Nginx Production Environment Variables
# This file contains nginx-specific environment variables for production
# It overrides values from config/defaults.env

# Virtual host configuration
VIRTUAL_HOST=statex.cz
DEFAULT_HOST=statex.cz

# SSL configuration
ALLOW_SELF_SIGNED=false

# Let's Encrypt configuration
LETSENCRYPT_EMAIL=admin@statex.cz

# Nginx-specific settings
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443
