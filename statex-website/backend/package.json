{
  "name": "statex-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "update-deps": "npx npm-check-updates -u && npm install",
    "update-deps:check": "npx npm-check-updates",
    "update-deps:interactive": "npx npm-check-updates -i"
  },
  "dependencies": {
    "@fastify/cors": "11.1.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/static": "8.2.0",
    "@prisma/client": "6.16.3",
    "@types/node": "24.7.0",
    "@types/uuid": "^11.0.0",
    "bcryptjs": "3.0.2",
    "bullmq": "5.60.0",
    "dotenv": "17.2.3",
    "fastify": "5.6.1",
    "ioredis": "5.8.1",
    "jsonwebtoken": "9.0.2",
    "prisma": "6.16.3",
    "typescript": "5.9.3",
    "uuid": "^13.0.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "3.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/supertest": "6.0.3",
    "supertest": "7.1.4",
    "tsx": "4.20.6",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=23.11.0",
    "npm": ">=11.5.2"
  }
}
